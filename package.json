  "license": "MIT",
  "private": true,
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "cp-index": "cp src/index.html dist",
    "cp-favicon": "cp src/images/favicon.png dist",
