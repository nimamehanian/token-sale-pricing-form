    "react-router-redux": "5.0.0-alpha.8",
    "react-text-mask": "5.1.0",
    "redux": "3.7.2",
    "redux-observable": "0.18.0",
    "rxjs": "5.5.6"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
